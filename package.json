{
    "name": "graphql-template-ts-mongo",
    "version": "1.0.0",
    "description": "This is a project template with Graphql, Apollo and mongodb",
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=production node --require dotenv/config 'dist/main.js'",
        "build": "ttsc --build tsconfig.json && copyfiles -f ./src/graphql/schemas/*.graphql ./src/graphql/schemas/*.gql ./dist/graphql/schemas",
        "start:dev": "npm run build:dev",
        "build:dev": "nodemon -r tsconfig-paths/register --require dotenv/config 'src/main.ts' --exec 'ts-node' src/main.ts -e ts,graphql",
        "lint": "eslint '{__mocks__,src,test}/**/*.{js,ts}'",
        "lint:fix": "eslint --fix '{__mocks__,src,test}/**/*.{js,ts}'",
        "test": "jest --coverage",
        "test:watch": "jest --coverage --watchAll"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.3",
        "@types/graphql": "^14.5.0",
        "@types/graphql-depth-limit": "^1.1.2",
        "@types/jest": "^25.1.3",
        "@types/mongoose": "^5.7.3",
        "@types/node": "^13.7.7",
        "@typescript-eslint/eslint-plugin": "^2.22.0",
        "@typescript-eslint/parser": "^2.22.0",
        "@zerollup/ts-transform-paths": "^1.7.12",
        "copyfiles": "^2.2.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "graphql-depth-limit": "^1.1.0",
        "graphql-import": "^1.0.0-beta.2",
        "graphql-import-node": "^0.0.4",
        "jest": "^25.1.0",
        "ncp": "^2.0.0",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "ts-jest": "^25.2.1",
        "ts-node": "^8.6.2",
        "tsc": "^1.20150623.0",
        "ttypescript": "^1.5.10",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@google-cloud/logging-winston": "^3.0.6",
        "@graphql-toolkit/file-loading": "^0.9.7",
        "@graphql-toolkit/schema-merging": "^0.9.7",
        "@typegoose/typegoose": "^6.4.0",
        "@types/lodash": "^4.14.149",
        "apollo-server": "^2.11.0",
        "apollo-server-express": "^2.11.0",
        "clear": "^0.1.0",
        "cli-block": "^1.1.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "graphql": "^14.6.0",
        "graphql-tools": "^4.0.7",
        "http": "^0.0.0",
        "import-modules": "^2.0.0",
        "listr": "^0.14.3",
        "lodash": "^4.17.15",
        "moment": "^2.27.0",
        "moment-timezone": "^0.5.31",
        "mongoose": "^5.9.3",
        "node-emoji": "^1.10.0",
        "tsconfig-paths": "^3.9.0",
        "winston": "^3.3.3"
    }
}
